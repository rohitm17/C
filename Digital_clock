#include<stdio.h>
#include<conio.h>
void main()
{int h=0,m=0,s=0,i=0;
clrscr();
printf("enter the value of hour ,minutes,seconds");
scanf("%d %d %d",&h,&m,&s);
start:;
for(h=h;h<24;h++)
{for(m=m;m<60;m++)
  {for(s=s;s<60;s++)
    clrscr();
      {printf("\n\n\n\n\n\n\n\n\n\t\t\t\t\%d : %d : %d\n",h,m,s);
	for(double i=0;i<99999999;i++)
	 {i++;
	  i--;}
	}
s=0;}
m=0;}
h=0;
goto start;
}
